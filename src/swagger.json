{
  "openapi": "3.0.0",
  "info": {
    "title": "RentalX Documentation",
    "description": "This is an API Rente",
    "version": "1.0.0",
    "contact": "igorbarbosaemerick@gmail.com"
  },
  "paths": {
    "/categories": {
      "post": {
        "tags": [
          "Category"
        ],
        "sumary": "Create a category",
        "description": "Create a new non exists category",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  }
                },
                "example": {
                  "name": "Category name sample",
                  "description": "Category description sample"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Category created sucess",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "name": {
                      "type": "string"
                    },
                    "description": {
                      "type": "string"
                    },
                    "created_at": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Category already exists"
          }
        }
      },
      "get": {
        "tags": [
          "Category"
        ],
        "sumary": "List categories",
        "description": "List all categories created",
        "responses": {
          "200": {
            "description": "Returns a array with all categories",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "description": {
                        "type": "string"
                      },
                      "created_at": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/categories/import": {
      "post": {
        "tags": [
          "Category"
        ],
        "sumary": "Create categories",
        "description": "Create categories using a file.csv",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": "file"
                },
                "example": {
                  "file": "File.csv"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Categories created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "description": {
                        "type": "string"
                      },
                      "created_at": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/specifications": {
      "post": {
        "tags": [
          "Specification"
        ],
        "sumary": "Create specification",
        "description": "Create a specification",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  }
                },
                "example": {
                  "name": "Specification name sample",
                  "description": "Specification description sample"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Specification created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "name": {
                      "type": "string"
                    },
                    "description": {
                      "type": "string"
                    },
                    "created_at": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "Specification"
        ],
        "sumary": "List specifications",
        "description": "List all specifications created",
        "responses": {
          "200": {
            "description": "Returns a array with all specifications",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "description": {
                        "type": "string"
                      },
                      "created_at": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}